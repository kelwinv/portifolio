{
  "extends": ["eslint:recommended", "next/core-web-vitals", "next/typescript"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "warn"
  },
  "overrides": [
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {
        "@typescript-eslint/no-unused-vars": [
          "warn",
          { "argsIgnorePattern": "^_", "varsIgnorePattern": "^_" }
        ]
      }
    }
  ]
}
